<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="log_in">
        <item name="android:textSize">30sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="title">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="button">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#212121</item>
    </style>

    <style name="forgot_pass">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="don_t_have_">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#B3000000</item>
    </style>
    <style name="or_login_wi">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#B3000000</item>
    </style>
    <style name="myEditTextLayout" parent="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="hintTextColor">@color/black</item>
        <item name="boxStrokeColor">@color/black</item>
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="android:textColorHint">@color/black</item>
    </style>
    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
    <style name="order_button"></style>
